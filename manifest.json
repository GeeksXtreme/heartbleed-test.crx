{
	"manifest_version": 2,
	"name": "Heartbleed Test",
	"version": "0.6.0",
	"description": "Test websites for Heartbleed vulnerability (CVE 2014-0160)",
	"author": "Pwnicorn",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"background": {
		"scripts": [ "background.js" ],
		"persistent": false
	},
	"content_scripts": [{
		"matches": [ "https://*/*", "http://*/*" ],
		"js": [ "content.js" ]
	}],
	"page_action": {
		"default_icon": {
			"19": "images/grey19.png",
			"38": "images/grey38.png"
		},
		"default_title": "Heartbleed Test"
	}
}

